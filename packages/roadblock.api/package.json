{
  "name": "@zthun/roadblock.api",
  "version": "1.0.0",
  "description": "This is the roadblock api used to get cookie tokens for the user on the given domain.",
  "author": "Anthony Bonta",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zthun/roadblock",
    "directory": "packages/roadblock.api"
  },
  "private": true,
  "main": "./dist/main.js",
  "bin": {
    "zthun-roadblock": "./bin/main.js"
  },
  "engines": {
    "node": "^16.6.0",
    "npm": "^7.19.0"
  },
  "scripts": {
    "docker:publish": "docker push zthun/roadblock.api && docker push zthun/roadblock.api:$(node -p \"require('./package.json').version\")",
    "docker:build": "docker build . -t zthun/roadblock.api:latest -t zthun/roadblock.api:$(node -p \"require('./package.json').version\")",
    "debug": "nodemon",
    "build": "tsc -p ./tsconfig.prod.json",
    "start": "node ./dist/main.js"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.4",
    "@nestjs/core": "^8.2.4",
    "@nestjs/microservices": "^8.2.4",
    "@nestjs/platform-express": "^8.2.4",
    "@zthun/works.core": "^3.0.0",
    "@zthun/works.microservices": "^3.0.0",
    "@zthun/works.nest": "^3.0.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.4.0"
  },
  "files": [
    "dist/**",
    "docs/**",
    "images/**"
  ],
  "sideEffects": false
}
